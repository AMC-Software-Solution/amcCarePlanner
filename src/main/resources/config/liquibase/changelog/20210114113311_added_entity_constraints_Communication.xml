        <addForeignKeyConstraint baseColumnNames="communicated_by_id"
                                 baseTableName="communication"
                                 constraintName="fk_communication_communicated_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="employee"/>

    </changeSet>
</databaseChangeLog>
